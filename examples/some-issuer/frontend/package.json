    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-social-login-buttons": "^3.9.1",
    "react-telegram-login": "^1.1.2"
